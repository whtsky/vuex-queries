{
  "name": "vuex-queries",
  "version": "1.1.1",
  "description": "Vuex-Queries helps you write query functions in Vuex",
  "main": "index.js",
  "repository": "git@github.com:whtsky/vuex-queries.git",
  "author": "Wu Haotian <whtsky@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/whtsky/vuex-queries/issues"
  },
  "homepage": "https://github.com/whtsky/vuex-queries",
  "scripts": {
    "prepack": "standard && npm run build",
    "build": "cross-env BABEL_ENV=production babel src/index.js -o index.js",
    "test": "cross-env NODE_ENV=test nyc ava"
  },
  "files": [
    "index.js"
  ],
  "pre-commit": [
    "test"
  ],
  "peerDependencies": {
    "vuex": "> 2.0 < 4"
  },
  "devDependencies": {
    "ava": "^0.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.1",
    "istanbul": "^0.4.5",
    "nyc": "^11.0.3",
    "pkg-ok": "^2.0.0",
    "pre-commit": "^1.2.2",
    "sinon": "^4.0.1",
    "standard": "^10.0.2",
    "vue": "^2.4.2",
    "vuex": "^3.0.1"
  },
  "ava": {
    "files": [
      "test.js"
    ],
    "source": [
      "src/index.js"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  }
}
